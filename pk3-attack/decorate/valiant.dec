	SummonMonsters:
		DIA2 A 16 Bright
		DIA2 A 0 A_SetUserVar("user_spawned", user_spawned + 1)
		DIA2 A 0 A_JumpIf(user_spawned <= 4, "SpawnShot1")
		DIA2 A 0 A_JumpIf(user_spawned <= 6, "SpawnShot2")
		DIA2 A 0 A_JumpIf(user_spawned <= 8, "SpawnShot3") //from valve.wad
